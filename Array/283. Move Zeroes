public class Solution {
    public void moveZeroes(int[] nums) {
        if(nums == null || nums.length == 0)
            return;

        int currentPos = 0;
        for(int num : nums){
            if(num != 0)
                nums[currentPos++] = num;
            }

        while(currentPos < nums.length){
            nums[currentPos++] = 0;
        }
        }
    }
